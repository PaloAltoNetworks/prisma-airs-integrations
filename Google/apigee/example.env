# Apigee Configuration
APIGEE_ORG="your-apigee-org"
APIGEE_ENV="eval"  # or test, prod

# Prisma AIRS Configuration
PANW_PRISMA_AIRS_API_KEY="your-airs-api-key-here"
AIRS_API_PROFILE_NAME="default"

# GCP/Vertex AI Configuration
GOOGLE_CLOUD_PROJECT="your-gcp-project-id"
VERTEX_MODEL="enabled model in VertexAI model garden"

# Deployment Service Account (path to JSON key file)
GOOGLE_APPLICATION_CREDENTIALS="/path/to/service-account-key.json"

# Optional: Skip IAM grant during deployment (set to 1 to skip)
# SKIP_IAM_GRANT=1
